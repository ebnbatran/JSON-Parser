#include <cmath>

namespace JSON {

    namespace Utility {
        
      template<typename T>
        T cton(const char &c) {
          switch (c) {
            case '0': return (T)0; break;
            case '1': return (T)1; break;
            case '2': return (T)2; break;
            case '3': return (T)3; break;
            case '4': return (T)4; break;
            case '5': return (T)5; break;
            case '6': return (T)6; break;
            case '7': return (T)7; break;
            case '8': return (T)8; break;
            case '9': return (T)9; break;
          }
        }
    };
};
